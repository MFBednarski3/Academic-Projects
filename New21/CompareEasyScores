
import java.util.Comparator;
//This compares the high scores for the easy level
public class CompareEasyScores implements Comparator<HighScoresEasy> {
	public int compare(HighScoresEasy score1, HighScoresEasy score2) {

		int firstScore = score1.getScore();
		int secScore = score2.getScore();

		if (firstScore > secScore) {
			return -1;
		} else if (firstScore < secScore) {
			return 1;
		} else {
			return 0;
		}
	}
}
