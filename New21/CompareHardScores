
import java.util.Comparator;
//Same for the Hard level
public class CompareHardScores implements Comparator<HighScoresHard> {
	public int compare(HighScoresHard score1, HighScoresHard score2) {

		int firstScore = score1.getScore();
		int secScore = score2.getScore();

		if (firstScore > secScore) {
			return -1;
		} else if (firstScore < secScore) {
			return 1;
		} else {
			return 0;
		}
	}
}
